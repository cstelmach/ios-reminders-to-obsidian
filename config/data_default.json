{
  "fundamentals": {
    "obsidianSettingsPath": null,
    "usePeriodicNotesPlugin": false,
    "dailyNoteFolderOverwrite": null,
    "dailyNoteFilenameOverwrite": null,
    "templateOverwritePath": null,
    "manualDatabaseFolderPath": null,
    "useDatabaseFunctions": true
  },
  "export": {
    "isCacheActive": true
  },
  "sections": {
    "sectionsToHide": [],
    "sectionsToAddAsTags": [
      {
        "sectionMatchRegex": ".*OBE.*",
        "tagToAddUponMatch": "OBE"
      }
    ]
  },
  "lists": {
    "listsToImport": [],
    "importAllListsButOmitted": false,
    "listsToOmit": []
  },
  "checkboxes": {
    "partiallyCompletedCheckbox": "[-]",
    "cancelledCheckbox": "[~]",
    "tagDependentCompletionKinds": {
      "cancelled": ["OBE", "CANCELLED"],
      "partiallyComplete": ["partiallyComplete"]
    }
  },
  "formatting": {
    "dateFormat": "YYYY-MM-DD",
    "timeFormat": "HH:mm:SS",
    "dateTimeSeparator": " ",
    "exportHeader": "Completed Tasks",
    "exportHeaderLevel": 2,
    "listHeaderLevel": 3,
    "wrapDateStringInInternalLink": false,
    "useMetadataFormattingForDatetimes": false,
    "useMetadataFormattingForSections": false,
    "useMetadataFormattingForPriority": false
  },
  "development": {
    "includeTodaysCompletedTasks": false,
    "skipNotesAlreadyImported": true
  }
}
