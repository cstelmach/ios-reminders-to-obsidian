{
  "fundamentals": {
    "obsidianSettingsPath": "/Users/cs/Obsidian/_/.obsidian",
    "usePeriodicNotesPlugin": true,
    "dailyNoteFolderOverwrite": "/Users/cs/local/code/python_modules/ios_reminders_to_markdown_journal/temp",
    "dailyNoteFilenameOverwrite": "JD YYYYMMDD",
    "templateOverwritePath": "/Users/cs/Obsidian/_/slipbox/resources/templates/RT Journal-Day-Only_Completed_Tasks.md",
    "manualDatabaseFolderPath": "/Users/cs/Library/Group Containers/group.com.apple.reminders/Container_v1/Stores/",
    "useDatabaseFunctions": true
  },
  "export": {
    "isCacheActive": true,
    "exportToCSV": true,
    "csvExportFolderPath": "/Users/cs/local/code/python_modules/ios_reminders_to_markdown_journal/temp",
    "exportToJSON": true,
    "jsonExportFolderPath": "/Users/cs/local/code/python_modules/ios_reminders_to_markdown_journal/temp"
  },
  "sections": {
    "sectionsToHide": ["Hidden Section", ".*_hidden"],
    "sectionsToAddAsTags": [
      {
        "sectionMatchRegex": ".*OBE.*",
        "tagToAddUponMatch": "OBE"
      },
      {
        "sectionMatchRegex": "Section1",
        "tagToAddUponMatch": "Section1Tag"
      }
    ]
  },
  "lists": {
    "listsToImport": [".*"],
    "importAllListsButOmitted": false,
    "listsToOmit": ["projects.code"]
  },
  "checkboxes": {
    "partiallyCompletedCheckbox": "[-]",
    "cancelledCheckbox": "[~]",
    "tagDependentCompletionKinds": {
      "cancelled": ["OBE", "CANCELLED"],
      "partiallyComplete": ["partiallyComplete"]
    },
    "sectionDependentCompletionKinds": {
      "cancelled": ["OBE"],
      "partiallyComplete": ["Section1"]
    }
  },
  "formatting": {
    "dateFormat": "JD YYYYMMDD",
    "timeFormat": "HH:mm",
    "dateTimeSeparator": " ",
    "exportHeader": "Completed Tasks",
    "exportHeaderLevel": 2,
    "listHeaderLevel": 3,
    "wrapDateStringInInternalLink": true,
    "formatAdvancedPropertiesInSingleLine": true,
    "useMetadataFormattingForDatetimes": true,
    "useMetadataFormattingForSections": true,
    "useMetadataFormattingForPriority": true,
    "useMetadataFormattingForTags": true,
    "removeNonStandardCharactersFromSectionNames": true
  },
  "development": {
    "includeTodaysCompletedTasks": true,
    "skipNotesAlreadyImported": false
  }
}
